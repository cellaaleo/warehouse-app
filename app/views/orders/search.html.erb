<h2>Resultados da busca por: <%= @code %></h2>

<p> 
  <%= @orders.count %> 
  <%= Order.model_name.human(count: @orders.count).downcase %> 
  <%# I18n.translate(:found, count: @orders.count) %>
  <%# I18n.t(:found, count: @orders.count) %>
  <%= t(:found, count: @orders.count) %>
</p>

<% @orders.each do |order| %>
  <%= render order %>
<% end %>